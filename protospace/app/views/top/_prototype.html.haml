.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag("#{prototype.prototype_photos.main.first.image}"), prototype_path(prototype)
    .caption
      %h3 #{prototype.title}
      .proto-meta
        .proto-user
          = link_to "by #{prototype.user.username}", user_path(prototype.user)
        .proto-posted
          Apr 26
      %ul.proto-tag-list.list-inline
        %li 

          = link_to "iPad", class: "btn btn-default"
        %li 
          = link_to "wireframe", class: "btn btn-default"
